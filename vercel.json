{
	"version": 2,
	"alias": "nuro.dev",
	"cleanUrls": true,
	"public": true,
	"regions": ["lhr1"],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Permissions-Policy",
					"value": "interest-cohort=()"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/github",
			"destination": "https://github.com/nurodev"
		},
		{
			"source": "/linkedin",
			"destination": "https://www.linkedin.com/in/nurodev"
		},
		{
			"source": "/twitter",
			"destination": "https://twitter.com/nurodev"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}
