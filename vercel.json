{
	"version": 2,
	"public": true,
	"alias": "nuro.dev",
	"regions": ["lhr1"],
	"functions": {
		"api/blog/posts.ts": {
			"includeFiles": "src/content/blog/**"
		}
	},
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/$1",
			"headers": {
				"Permissions-Policy": "interest-cohort=()"
			}
		},
		{
			"src": "/github",
			"dest": "https://github.com/nurodev"
		},
		{
			"src": "/linkedin",
			"dest": "https://www.linkedin.com/in/nurodev"
		},
		{
			"src": "/twitter",
			"dest": "https://twitter.com/nurodev"
		}
	]
}
