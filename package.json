{
	"private": true,
	"name": "nuro.dev",
	"version": "2.1.1",
	"description": "Personal Website for nuro.dev",
	"repository": "https://github.com/nurodev/nuro.dev.git",
	"author": "nurodev <me@nuro.dev>",
	"license": "MIT",
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"dev": "concurrently -n client,server \"yarn dev:client\" \"yarn dev:server\"",
		"dev:client": "cross-env DEBUG=vite-ssg:* vite",
		"dev:server": "vercel dev --listen 3030",
		"build": "vue-tsc --noEmit && cross-env NODE_ENV=production vite-ssg build",
		"preview": "vite preview",
		"prettier": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx,vue}\""
	},
	"dependencies": {
		"@headlessui/vue": "^1.3.0",
		"@splitbee/web": "^0.2.4",
		"@vueuse/core": "^5.2.0",
		"@vueuse/head": "^0.5.1",
		"@vueuse/sound": "^1.1.7",
		"date-fns": "^2.22.1",
		"front-matter": "^4.0.2",
		"inter-ui": "^3.19.2",
		"nprogress": "^0.2.0",
		"ogl-typescript": "^0.1.40",
		"vite-ssg": "^0.12.0",
		"vue": "^3.1.5",
		"web-vitals": "^2.1.0"
	},
	"devDependencies": {
		"@iconify/json": "^1.1.380",
		"@types/howler": "^2.2.3",
		"@types/markdown-it-emoji": "^2.0.1",
		"@types/node": "^16.4.4",
		"@types/node-fetch": "^2.5.12",
		"@types/nprogress": "^0.2.0",
		"@vercel/node": "^1.11.1",
		"@vitejs/plugin-vue": "^1.3.0",
		"@vue/compiler-sfc": "^3.1.5",
		"@vue/server-renderer": "^3.1.5",
		"concurrently": "^6.2.0",
		"cross-env": "^7.0.3",
		"diacritics": "^1.3.0",
		"markdown-it-anchor": "^7.1.0",
		"markdown-it-emoji": "^2.0.0",
		"markdown-it-link-attributes": "^3.0.0",
		"markdown-it-prism": "^2.1.6",
		"node-fetch": "^2.6.1",
		"prismjs": "^1.23.0",
		"typescript": "^4.3.5",
		"vite": "^2.4.4",
		"vite-plugin-components": "^0.13.2",
		"vite-plugin-icons": "^0.6.5",
		"vite-plugin-md": "^0.9.0",
		"vite-plugin-pages": "^0.15.1",
		"vite-plugin-pwa": "^0.8.2",
		"vite-plugin-restart": "^0.0.2",
		"vite-plugin-windicss": "^1.2.7",
		"vue-router": "^4.0.8",
		"vue-tsc": "^0.2.2",
		"windicss": "^3.1.6"
	}
}
